//<h5> Determine Divisiblity <h5>

//<p> write a function
// input:
// array = int[]
// x = int
// return: bool[] where each element represent whether element in array is divisible by x
// example:
// input:
// array = [5, 6, 40, 30, 72]
// x = 3
// output: [false, true, false, true, true]</p>

const determineDivisbility = (array, x) => {
 return array.map(y => y % x == 0)
}
console.log(determineDivisbility([5, 6, 40, 30, 72], 3))
