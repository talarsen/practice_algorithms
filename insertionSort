arr = [1, 2, 3, 4, 6, 7, 8, 9, 10, 5]
n = 10

const  insertionSort1 = (arr, n)  => {
let numToInsert = arr[arr.length -1 ];
  
  for(let i = n -2; i >=0;  i--){ 
    if(numToInsert < arr[i]){
      arr[i + 1] = arr[i]
      console.log(arr.join(" "));
    }
    else {
      arr[i+ 1] = numToInsert;
      console.log(arr.join(" "));
      break;
    }
    if (i === 0) {
        arr[i + i] = numToInsert;
        console.log(arr.join(" "));
    }
  }
}
insertionSort1(arr, n)
